<?xml version="1.0" encoding="UTF-8"?>
<jcr:root xmlns:sling="http://sling.apache.org/jcr/sling/1.0"
          xmlns:granite="http://www.adobe.com/jcr/granite/1.0"
          xmlns:cq="http://www.day.com/jcr/cq/1.0"
          xmlns:jcr="http://www.jcp.org/jcr/1.0"
          xmlns:nt="http://www.jcp.org/jcr/nt/1.0"
          jcr:primaryType="nt:unstructured"
          jcr:title="Test Component"
          sling:resourceType="cq/gui/components/authoring/dialog"
          height="{Double}480.0"
          width="{Double}560.0">
    <content
            jcr:primaryType="nt:unstructured"
            sling:resourceType="granite/ui/components/coral/foundation/container">
        <items jcr:primaryType="nt:unstructured">
            <tabs
                    jcr:primaryType="nt:unstructured"
                    sling:resourceType="granite/ui/components/coral/foundation/tabs">
                <items jcr:primaryType="nt:unstructured">
                    <firstTab jcr:primaryType="nt:unstructured"
                              sling:resourceType="granite/ui/components/coral/foundation/container"
                              jcr:title="First tab"
                              granite:hidden="{Boolean}false">
                        <items jcr:primaryType="nt:unstructured">
                            <firstFieldPrimaryField
                                    jcr:primaryType="nt:unstructured"
                                    sling:resourceType="cq/gui/components/authoring/dialog/fileupload"
                                    allowUpload="{Boolean}false"
                                    chunkUploadSupported="{Boolean}false"
                                    disabled="{Boolean}false"
                                    fieldDescription="Description to first primary field"
                                    fieldLabel="Label to first primary field"
                                    mimeTypes="[image]"
                                    multiple="{Boolean}false"
                                    name="./primary1First_Field_Primary_Field"
                                    renderHidden="{Boolean}false"
                                    required="{Boolean}true"
                                    text="Upload"
                                    uploadUrl="${suffix.path}"
                                    useHTML5="{Boolean}true"
                                    variant="secondary"/>
                            <secondFieldPrimaryField
                                    jcr:primaryType="nt:unstructured"
                                    sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                    disabled="{Boolean}false"
                                    fieldDescription="Description to second primary field"
                                    fieldLabel="Label to second primary field"
                                    name="./primary1Second_Field_Primary_Field"
                                    renderHidden="{Boolean}false"
                                    required="{Boolean}true"/>
                            <title
                                    jcr:primaryType="nt:unstructured"
                                    sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                    disabled="{Boolean}false"
                                    fieldDescription="title's description"
                                    fieldLabel="title's label"
                                    name="./primary1title"
                                    renderHidden="{Boolean}false"
                                    required="{Boolean}true"/>
                            <description
                                    jcr:primaryType="nt:unstructured"
                                    sling:resourceType="cq/gui/components/authoring/dialog/richtext"
                                    disabled="{Boolean}false"
                                    fieldDescription="Description"
                                    fieldLabel="Description's label"
                                    name="./primary1description"
                                    renderHidden="{Boolean}false"
                                    required="{Boolean}true"
                                    useFixedInlineToolbar="{Boolean}true">
                                <uiSettings jcr:primaryType="nt:unstructured">
                                    <cui jcr:primaryType="nt:unstructured">
                                        <inline
                                                jcr:primaryType="nt:unstructured"
                                                toolbar="[links#modifylink,links#unlink,subsuperscript#subscript,subsuperscript#superscript]"/>
                                        <fullscreen
                                                jcr:primaryType="nt:unstructured"
                                                toolbar="[links#modifylink,links#unlink,subsuperscript#subscript,subsuperscript#superscript]"/>
                                        <dialogFullScreen
                                                jcr:primaryType="nt:unstructured"
                                                toolbar="[links#modifylink,links#unlink,subsuperscript#subscript,subsuperscript#superscript]"/>
                                    </cui>
                                </uiSettings>
                                <rtePlugins jcr:primaryType="nt:unstructured">
                                    <subsuperscript
                                            jcr:primaryType="nt:unstructured"
                                            features="[subscript,superscript]"/>
                                    <links
                                            jcr:primaryType="nt:unstructured"
                                            features="[unlink,modifylink]"/>
                                    <styles jcr:primaryType="nt:unstructured"/>
                                </rtePlugins>
                            </description>
                            <fieldEnabled
                                    jcr:primaryType="nt:unstructured"
                                    sling:resourceType="granite/ui/components/coral/foundation/form/switch"
                                    checked="{Boolean}false"
                                    disabled="{Boolean}false"
                                    fieldDescription="Field enable description"
                                    fieldLabel="Field enable label"
                                    ignoreData="{Boolean}false"
                                    name="./primary1fieldEnable"
                                    offText="Off"
                                    onText="On"
                                    renderHidden="{Boolean}false"
                                    required="{Boolean}false"
                                    uncheckedValue="{Boolean}false"
                                    value="{Boolean}true"/>
                            <reference
                                    jcr:primaryType="nt:unstructured"
                                    sling:resourceType="granite/ui/components/coral/foundation/form/radiogroup"
                                    disabled="{Boolean}false"
                                    fieldDescription="Description for the reference field."
                                    fieldLabel="reference name"
                                    name="./primary1reference_name"
                                    renderHidden="{Boolean}false"
                                    required="{Boolean}true"
                                    vertical="{Boolean}true">
                                <items jcr:primaryType="nt:unstructured">
                                    <basic
                                            jcr:primaryType="nt:unstructured"
                                            checked="{Boolean}true"
                                            text="Basic"
                                            value="basic"/>
                                    <detailed
                                            jcr:primaryType="nt:unstructured"
                                            text="Detailed"
                                            value="detailed"/>
                                </items>
                            </reference>
                            <firstField
                                    jcr:primaryType="nt:unstructured"
                                    sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                    disabled="{Boolean}false"
                                    fieldDescription="Description for first field"
                                    fieldLabel="Label to first field"
                                    name="./primary1first_field"
                                    renderHidden="{Boolean}false"
                                    required="{Boolean}true"/>
                            <secondField
                                    jcr:primaryType="nt:unstructured"
                                    sling:resourceType="granite/ui/components/coral/foundation/form/pathfield"
                                    disabled="{Boolean}false"
                                    fieldDescription="Description for second field"
                                    fieldLabel="Label to second field"
                                    name="./primary1second_field"
                                    renderHidden="{Boolean}false"
                                    required="{Boolean}true"
                                    rootPath="/"/>
                            <thirdField
                                    jcr:primaryType="nt:unstructured"
                                    sling:resourceType="granite/ui/components/coral/foundation/form/checkbox"
                                    autosubmit="{Boolean}false"
                                    checked="{Boolean}false"
                                    disabled="{Boolean}false"
                                    fieldDescription="Description for third field"
                                    fieldLabel="Label to third field"
                                    name="./primary1third_field"
                                    renderHidden="{Boolean}false"
                                    required="{Boolean}false"
                                    text="Label to third field"
                                    uncheckedValue="{Boolean}false"
                                    value="{Boolean}true"/>
                        </items>
                    </firstTab>
                    <secondTab jcr:primaryType="nt:unstructured"
                               sling:resourceType="granite/ui/components/coral/foundation/container"
                               jcr:title="Second tab"
                               granite:hidden="{Boolean}false">
                        <items jcr:primaryType="nt:unstructured">
                            <secondPrimaryDialog
                                    jcr:primaryType="nt:unstructured"
                                    sling:resourceType="granite/ui/components/coral/foundation/form/fieldset"
                                    disabled="{Boolean}false"
                                    name="./secondPrimaryDialog"
                                    renderHidden="{Boolean}false"
                                    required="{Boolean}false">
                                <items jcr:primaryType="nt:unstructured">
                                    <firstFieldPrimaryField
                                            jcr:primaryType="nt:unstructured"
                                            sling:resourceType="cq/gui/components/authoring/dialog/fileupload"
                                            allowUpload="{Boolean}false"
                                            chunkUploadSupported="{Boolean}false"
                                            disabled="{Boolean}false"
                                            fieldDescription="Description to first primary field"
                                            fieldLabel="Label to first primary field"
                                            mimeTypes="[image]"
                                            multiple="{Boolean}false"
                                            name="./primary2First_Field_Primary_Field"
                                            renderHidden="{Boolean}false"
                                            required="{Boolean}true"
                                            text="Upload"
                                            uploadUrl="${suffix.path}"
                                            useHTML5="{Boolean}true"
                                            variant="secondary"/>
                                    <secondFieldPrimaryField
                                            jcr:primaryType="nt:unstructured"
                                            sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                            disabled="{Boolean}false"
                                            fieldDescription="Description to second primary field"
                                            fieldLabel="Label to second primary field"
                                            name="./primary2Second_Field_Primary_Field"
                                            renderHidden="{Boolean}false"
                                            required="{Boolean}true"/>
                                    <title
                                            jcr:primaryType="nt:unstructured"
                                            sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                            disabled="{Boolean}false"
                                            fieldDescription="title's description"
                                            fieldLabel="title's label"
                                            name="./primary2title"
                                            renderHidden="{Boolean}false"
                                            required="{Boolean}true"/>
                                    <description
                                            jcr:primaryType="nt:unstructured"
                                            sling:resourceType="cq/gui/components/authoring/dialog/richtext"
                                            disabled="{Boolean}false"
                                            fieldDescription="Description"
                                            fieldLabel="Description's label"
                                            name="./primary2description"
                                            renderHidden="{Boolean}false"
                                            required="{Boolean}true"
                                            useFixedInlineToolbar="{Boolean}true">
                                        <uiSettings jcr:primaryType="nt:unstructured">
                                            <cui jcr:primaryType="nt:unstructured">
                                                <inline
                                                        jcr:primaryType="nt:unstructured"
                                                        toolbar="[links#modifylink,links#unlink,subsuperscript#subscript,subsuperscript#superscript]"/>
                                                <fullscreen
                                                        jcr:primaryType="nt:unstructured"
                                                        toolbar="[links#modifylink,links#unlink,subsuperscript#subscript,subsuperscript#superscript]"/>
                                                <dialogFullScreen
                                                        jcr:primaryType="nt:unstructured"
                                                        toolbar="[links#modifylink,links#unlink,subsuperscript#subscript,subsuperscript#superscript]"/>
                                            </cui>
                                        </uiSettings>
                                        <rtePlugins jcr:primaryType="nt:unstructured">
                                            <subsuperscript
                                                    jcr:primaryType="nt:unstructured"
                                                    features="[subscript,superscript]"/>
                                            <links
                                                    jcr:primaryType="nt:unstructured"
                                                    features="[unlink,modifylink]"/>
                                            <styles jcr:primaryType="nt:unstructured"/>
                                        </rtePlugins>
                                    </description>
                                    <fieldEnabled
                                            jcr:primaryType="nt:unstructured"
                                            sling:resourceType="granite/ui/components/coral/foundation/form/switch"
                                            checked="{Boolean}false"
                                            disabled="{Boolean}false"
                                            fieldDescription="Field enable description"
                                            fieldLabel="Field enable label"
                                            ignoreData="{Boolean}false"
                                            name="./primary2fieldEnable"
                                            offText="Off"
                                            onText="On"
                                            renderHidden="{Boolean}false"
                                            required="{Boolean}false"
                                            uncheckedValue="{Boolean}false"
                                            value="{Boolean}true"/>
                                    <reference
                                            jcr:primaryType="nt:unstructured"
                                            sling:resourceType="granite/ui/components/coral/foundation/form/radiogroup"
                                            disabled="{Boolean}false"
                                            fieldDescription="Description for the reference field."
                                            fieldLabel="reference name"
                                            name="./primary2reference_name"
                                            renderHidden="{Boolean}false"
                                            required="{Boolean}true"
                                            vertical="{Boolean}true">
                                        <items jcr:primaryType="nt:unstructured">
                                            <basic
                                                    jcr:primaryType="nt:unstructured"
                                                    checked="{Boolean}true"
                                                    text="Basic"
                                                    value="basic"/>
                                            <detailed
                                                    jcr:primaryType="nt:unstructured"
                                                    text="Detailed"
                                                    value="detailed"/>
                                        </items>
                                    </reference>
                                    <firstField
                                            jcr:primaryType="nt:unstructured"
                                            sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                            disabled="{Boolean}false"
                                            fieldDescription="Description for first field"
                                            fieldLabel="Label to first field"
                                            name="./primary2first_field"
                                            renderHidden="{Boolean}false"
                                            required="{Boolean}true"/>
                                    <secondField
                                            jcr:primaryType="nt:unstructured"
                                            sling:resourceType="granite/ui/components/coral/foundation/form/pathfield"
                                            disabled="{Boolean}false"
                                            fieldDescription="Description for second field"
                                            fieldLabel="Label to second field"
                                            name="./primary2second_field"
                                            renderHidden="{Boolean}false"
                                            required="{Boolean}true"
                                            rootPath="/"/>
                                    <thirdField
                                            jcr:primaryType="nt:unstructured"
                                            sling:resourceType="granite/ui/components/coral/foundation/form/checkbox"
                                            autosubmit="{Boolean}false"
                                            checked="{Boolean}false"
                                            disabled="{Boolean}false"
                                            fieldDescription="Description for third field"
                                            fieldLabel="Label to third field"
                                            name="./primary2third_field"
                                            renderHidden="{Boolean}false"
                                            required="{Boolean}false"
                                            text="Label to third field"
                                            uncheckedValue="{Boolean}false"
                                            value="{Boolean}true"/>
                                </items>
                            </secondPrimaryDialog>
                        </items>
                    </secondTab>
                </items>
            </tabs>
        </items>
    </content>
</jcr:root>
